# Copy to config/runtime.env and populate values for local persistence.
# Do NOT commit actual secrets.

# Web search agent
WEB_SEARCH_PROVIDER=searxng
WEB_SEARCH_URL=http://searxng:8080
WEB_SEARCH_ENABLE=true
WEB_SEARCH_MAX_RESULTS=10
WEB_SEARCH_DOMAIN_LIMIT=5
WEB_SEARCH_DOMAIN_ALLOWLIST=cxc.org,moe.gov.tt,storage.moe.gov.tt,moey.gov.jm,education.govt.lc,moecst.gov.bz,mes.gov.bb,education.gov.lc,moe.gov.gy

